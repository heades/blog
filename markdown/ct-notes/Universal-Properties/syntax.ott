indexvar n, m, j, k ::=

grammar

  Objects, A, B, C, D, E, F, G, M, X, P, f, g, h :: 'Obj_'   ::=
    | r              ::   :: El
    | 1              ::   :: OneCat
    {{tex \mathbf{1} }}
    | Set            ::   :: Set
    {{tex \mathsf{Set} }}
    | Nat            ::   :: Nat
    {{tex \mathbb{N} }}
    | {*}            ::   :: SSet
    {{tex \{ \star \} }}
    | bullet         ::   :: Bullet
    {{tex \bullet }}
    | Cat A          ::   :: Cat
    {{tex \mathcal{[[A]]} }}
    | Catop A        ::   :: Catop
    {{tex \mathcal{[[A]]}^{\mathsf{op} } }}
    | obj ( A )      ::   :: obj
    {{tex [[obj]]([[A]]) }}
    | f ( g1 , ... , gj ) :: M :: App
    | f ( A1 , ... , Aj ) :: M :: App2
    | h{ A }              :: M :: HOParens
    {{tex [[A]] }}
    | e    :: :: MId
    {{tex e }}
    | i    :: :: Iid
    {{tex i}}
    | id A :: :: Id
    {{tex [[id]]_{[[A]]} }}
    | phi  :: :: Phi
    {{tex \phi }}
    | pi1  :: :: Pi1
    {{tex \pi_1 }}
    | pi2  :: :: Pi2
    {{tex \pi_2 }}
    | [ bifunctor ] :: :: Product
    {{tex [[bifunctor]] }}
    | f bifunctor g :: :: BifApp
    | { f }         :: :: Parens
    {{tex ( [[f]] ) }}

  bifunctor :: 'bifunctor_' ::=
    | (*) :: :: Oast
    | *   :: :: Otimes
    | (x) :: :: OTimes
    | ->  :: :: Arrow

  terminals :: 'terminals_' ::=
    | (*) :: :: Oast
    {{tex \oast }}
    | *   :: :: Otimes
    {{tex \times }}
    | (x) :: :: OTimes
    {{tex \otimes }}
    | ->  :: :: Mto
    {{tex \mto{} }}
    | id  :: :: Id
    {{tex \mathsf{id} }}
    | fst :: :: Fst
    {{tex \mathsf{fst} }}
    | snd :: :: Snd
    {{tex \mathsf{snd} }}
    | obj :: :: obj
    {{tex \mathsf{Obj} }}

  formula :: 'formula_' ::=
    | f : A -> B :: :: Mor